        </insert>
        <insert tableName="translation_variant">
            <column name="polish_meaning" value="marka"/>
        </insert>
        <insert tableName="translation_variant">
            <column name="polish_meaning" value="dać radę (np. dotrzeć gdzieś na czas)"/>
        </insert>
